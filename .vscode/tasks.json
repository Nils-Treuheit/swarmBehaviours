{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Run Sketch",
      "type": "shell",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "command": "C:/Program Files (x86)/Processing/processing-java.exe",
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated"
      },
      "args": [
        "--force",
        "--sketch=${workspaceRoot}/Java-Demo/swarmBehaviours",
        "--output=${workspaceRoot}/Java-Demo/swarmBehaviours/out",
        "--run"
      ],
      "windows": {
        "type": "process",
        "args": [
          "--force",
          {
            "value": "--sketch=${workspaceRoot}/Java-Demo/swarmBehaviours",
            "quoting": "strong"
          },
          {
            "value": "--output=${workspaceRoot}/Java-Demo/swarmBehaviours\\out",
            "quoting": "strong"
          },
          "--run"
        ]
      },
      "problemMatcher": []
    }
  ]
}
